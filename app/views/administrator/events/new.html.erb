<br />
<div class="booyah-box col-10 offset-1">
  <h1>Add a new event</h1>

  <%= simple_form_for @event, url: administrator_events_path do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />

</div>